{
  "extends": "../tslint.json",
  "rules": {
    "disable-next-line": "no-input-rename",
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ],

  }
}
